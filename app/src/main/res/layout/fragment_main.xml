<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:clipToPadding="true"
    android:orientation="vertical">

    <RelativeLayout
        android:visibility="gone"
        android:id="@+id/mainUI_titleLayout"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp40"
        android:background="@color/ui_main">

        <ImageView
            android:id="@+id/mainUI_leftBtn"
            android:layout_width="@dimen/dp45"
            android:layout_height="match_parent"
            android:background="@null"
            android:src="@drawable/main_left_icon"
            android:padding="8dp"
            android:layout_alignParentLeft="true"/>

        <TextView
            android:id="@+id/mainUI_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="7MA出行"
            android:textColor="@color/white"
            android:textSize="@dimen/sp16"/>

        <ImageView
            android:id="@+id/mainUI_rightBtn"
            android:layout_width="@dimen/dp45"
            android:layout_height="match_parent"
            android:background="@null"
            android:src="@drawable/main_right_icon"
            android:padding="@dimen/dp10"
            android:layout_alignParentRight="true"/>
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <View android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="#dcdcdc"></View>
        <com.flyco.tablayout.CommonTabLayout
            android:id="@+id/tab"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:background="#ffffff"
            app:tl_indicator_color="#F95A64"
            app:tl_indicator_height="2dp"
            app:tl_indicator_width="92dp"
            app:tl_textSelectColor="#FD555B"
            app:tl_textUnselectColor="#666666"
            app:tl_textsize="18sp"
            app:tl_underline_color="#DDDDDD"
            app:tl_underline_height="0dp"/>
    </LinearLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/ui_backgroud"
        android:layout_below="@id/mainUI_titleLayout"
        android:layout_weight="1">

        <com.amap.api.maps.MapView
            android:id="@+id/mainUI_map"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true">
        </com.amap.api.maps.MapView>

        <FrameLayout
            android:id="@+id/fl_change2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >
        </FrameLayout>


        <LinearLayout
            android:id="@+id/ll_top"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            >

            <LinearLayout
                android:id="@+id/rl_ad"
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:layout_marginLeft="18dp"
                android:layout_marginRight="18dp"
                android:layout_marginTop="12dp"
                android:background="@drawable/block_bcg"
                >


                <!--<ImageView-->
                <!--android:layout_width="match_parent"-->
                <!--android:layout_height="match_parent"-->
                <!--android:padding="3dp"-->
                <!--android:src="@drawable/ad"/>-->

                <com.youth.banner.Banner
                    android:id="@+id/banner"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    />

            </LinearLayout>

            <!--android:visibility="gone"-->

            <LinearLayout
                android:visibility="gone"
                android:id="@+id/ll_top_biking"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:background="@drawable/block_bcg"
                android:layout_marginTop="12dp"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="16dp"
                >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:layout_marginTop="7dp"
                    android:layout_marginBottom="4dp"
                    android:layout_marginLeft="0dp"
                    android:orientation="horizontal"
                    android:layout_alignParentLeft="true">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:layout_marginLeft="0dp"
                        android:orientation="vertical"
                        android:layout_alignParentLeft="true">

                        <TextView
                            android:id="@+id/tv_biking_codenum"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="0dp"
                            android:text="112233"
                            android:textColor="#333333"
                            android:textSize="@dimen/sp14"
                            />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="1dp"
                            android:text="单车编号"
                            android:textColor="#666666"
                            android:textSize="@dimen/sp14"
                            />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_estimated_cost"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:layout_marginLeft="0dp"
                        android:orientation="vertical"
                        android:layout_alignParentLeft="true"
                        >

                        <TextView
                            android:id="@+id/tv_estimated_cost"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="0dp"
                            android:text="¥1.00"
                            android:textColor="#FF0000"
                            android:textSize="@dimen/sp14"
                            />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="1dp"
                            android:text="预计费用"
                            android:textColor="#666666"
                            android:textSize="@dimen/sp14"
                            />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_electricity"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:layout_marginLeft="0dp"
                        android:orientation="vertical"
                        android:layout_alignParentLeft="true"
                        android:visibility="gone"
                        >

                        <TextView
                            android:id="@+id/tv_car_electricity"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="0dp"
                            android:text="83%"
                            android:textColor="#FF0000"
                            android:textSize="@dimen/sp14"
                            />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="1dp"
                            android:text="当前电量"
                            android:textColor="#666666"
                            android:textSize="@dimen/sp14"
                            />

                    </LinearLayout>


                </LinearLayout>


                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="14dp"
                    android:layout_marginRight="14dp"
                    android:background="#E5E5E5"
                    />

                <LinearLayout

                    android:id="@+id/ll_bike"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="12dp"
                    android:layout_marginBottom="21dp"
                    android:layout_marginLeft="30dp"
                    android:orientation="horizontal"
                    android:layout_alignParentLeft="true">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="1dp"
                        android:text="借车时间："
                        android:textColor="#717171"
                        android:textSize="@dimen/sp14"
                        />

                    <TextView
                        android:id="@+id/tv_car_start_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="0dp"
                        android:text="2019-2-20 12:00"
                        android:textColor="#3A3A3A"
                        android:textSize="@dimen/sp14"
                        />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_ebike"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="3dp"
                    android:layout_marginBottom="9dp"
                    android:layout_marginLeft="30dp"
                    android:orientation="vertical"
                    android:layout_alignParentLeft="true">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_alignParentLeft="true">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="1dp"
                            android:text="续航里程："
                            android:textColor="#717171"
                            android:textSize="@dimen/sp14"
                            />

                        <TextView
                            android:id="@+id/tv_car_mileage"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="0dp"
                            android:text="20KM"
                            android:textColor="#3A3A3A"
                            android:textSize="@dimen/sp14"
                            />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="6dp"
                        android:orientation="horizontal"
                        android:layout_alignParentLeft="true">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="1dp"
                            android:text="借车时间："
                            android:textColor="#717171"
                            android:textSize="@dimen/sp14"
                            />

                        <TextView
                            android:id="@+id/tv_car_start_time2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="0dp"
                            android:text="2019-2-20 12:00"
                            android:textColor="#3A3A3A"
                            android:textSize="@dimen/sp14"
                            />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="6dp"
                        android:orientation="horizontal"
                        android:layout_alignParentLeft="true">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="1dp"
                            android:text="预计费用："
                            android:textColor="#717171"
                            android:textSize="@dimen/sp14"
                            />

                        <TextView
                            android:id="@+id/tv_estimated_cost2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="0dp"
                            android:text="¥1.00"
                            android:textColor="#FF0000"
                            android:textSize="@dimen/sp14"
                            />
                    </LinearLayout>

                </LinearLayout>




                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="9dp"
                    android:orientation="horizontal"
                    >

                    <LinearLayout
                        android:id="@+id/ll_biking_openAgain"
                        android:layout_width="0dp"
                        android:layout_height="38dp"
                        android:layout_weight="1"
                        android:layout_marginLeft="14dp"
                        android:layout_marginRight="14dp"
                        android:orientation="horizontal"
                        android:gravity="center"
                        android:background="@drawable/btn_bcg_biking"
                        >

                        <TextView
                            android:id="@+id/tv_againBtn"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="再次开锁"
                            android:textSize="@dimen/sp14"
                            android:textColor="#FD555B"
                            />

                    </LinearLayout>


                    <LinearLayout
                        android:id="@+id/ll_biking_endBtn"
                        android:layout_width="0dp"
                        android:layout_height="40dp"
                        android:layout_weight="1"
                        android:layout_marginLeft="14dp"
                        android:layout_marginRight="14dp"
                        android:orientation="horizontal"
                        android:gravity="center"
                        android:background="@drawable/btn_bcg_biking2"
                        >

                        <TextView
                            android:id="@+id/tv_nextBtn2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="我要还车"
                            android:textSize="@dimen/sp16"
                            android:textColor="@color/white"
                            />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_biking_errorEnd"
                        android:layout_width="0dp"
                        android:layout_height="38dp"
                        android:layout_weight="1"
                        android:layout_marginLeft="14dp"
                        android:layout_marginRight="14dp"
                        android:orientation="horizontal"
                        android:gravity="center"
                        android:background="@drawable/btn_bcg_biking"
                        >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="无法还车"
                            android:textSize="@dimen/sp14"
                            android:textColor="#FD555B"
                            />

                    </LinearLayout>

                </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:visibility="gone"
                android:id="@+id/ll_top_pay"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:background="@drawable/block_bcg"
                android:layout_marginTop="12dp"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="16dp"
                >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:layout_marginTop="7dp"
                    android:layout_marginBottom="2dp"
                    android:orientation="horizontal"
                    android:layout_alignParentLeft="true">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:layout_marginLeft="0dp"
                        android:orientation="vertical"
                        android:layout_alignParentLeft="true">

                        <TextView
                            android:id="@+id/tv_pay_codenum"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="0dp"
                            android:text="112233"
                            android:textColor="#333333"
                            android:textSize="@dimen/sp14"
                            />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="1dp"
                            android:text="单车编号"
                            android:textColor="#666666"
                            android:textSize="@dimen/sp14"
                            />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_order_amount"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:layout_marginLeft="0dp"
                        android:orientation="vertical"
                        android:layout_alignParentLeft="true"
                        >

                        <TextView
                            android:id="@+id/tv_order_amount"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="0dp"
                            android:text="¥1.00"
                            android:textColor="#FF0000"
                            android:textSize="@dimen/sp14"
                            />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="1dp"
                            android:text="费用"
                            android:textColor="#666666"
                            android:textSize="@dimen/sp14"
                            />

                    </LinearLayout>

                </LinearLayout>


                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="14dp"
                    android:layout_marginRight="14dp"
                    android:background="#E5E5E5"
                    />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="7dp"
                    android:layout_marginLeft="26dp"
                    android:orientation="horizontal"
                    android:layout_alignParentLeft="true">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="1dp"
                        android:text="借车时间："
                        android:textColor="#717171"
                        android:textSize="@dimen/sp14"
                        />

                    <TextView
                        android:id="@+id/tv_pay_car_start_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="0dp"
                        android:text="2019-2-20 12:00"
                        android:textColor="#3A3A3A"
                        android:textSize="@dimen/sp14"
                        />
                </LinearLayout>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6dp"
                    android:layout_marginBottom="8dp"
                    android:layout_marginLeft="26dp"
                    android:orientation="horizontal"
                    android:layout_alignParentLeft="true">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="1dp"
                        android:text="还车时间："
                        android:textColor="#717171"
                        android:textSize="@dimen/sp14"
                        />

                    <TextView
                        android:id="@+id/tv_pay_car_end_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="0dp"
                        android:text="2019-2-20 12:00"
                        android:textColor="#3A3A3A"
                        android:textSize="@dimen/sp14"
                        />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="19dp"
                    android:orientation="horizontal"
                    >

                    <LinearLayout
                        android:id="@+id/ll_payBtn"
                        android:layout_width="0dp"
                        android:layout_height="40dp"
                        android:layout_weight="1"
                        android:layout_marginLeft="25dp"
                        android:layout_marginRight="25dp"
                        android:orientation="horizontal"
                        android:gravity="center"
                        android:background="@drawable/btn_bcg_biking2"
                        >

                        <TextView
                            android:id="@+id/tv_payBtn"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="骑行支付"
                            android:textSize="@dimen/sp16"
                            android:textColor="@color/white"
                            />

                    </LinearLayout>

                </LinearLayout>


            </LinearLayout>

            <RelativeLayout
                android:id="@+id/rl_authBtn"
                android:layout_width="match_parent"
                android:layout_height="32dp"
                android:layout_below="@+id/rl_ad"
                android:background="@drawable/block_bcg"
                android:layout_marginTop="7dp"
                android:layout_marginLeft="18dp"
                android:layout_marginRight="18dp">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="13dp"
                    android:orientation="horizontal"
                    android:layout_alignParentLeft="true">

                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="18dp"
                        android:src="@drawable/horn"/>

                    <TextView
                        android:id="@+id/tv_authBtn"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textColor="#EC3F4C"
                        android:textSize="@dimen/sp12"
                        android:layout_marginLeft="7dp"/>
                </LinearLayout>


                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:layout_marginRight="2dp"
                    android:orientation="horizontal"
                    android:layout_alignParentRight="true">

                    <ImageView
                        android:layout_width="7dp"
                        android:layout_height="12dp"
                        android:src="@drawable/yjt"/>
                </LinearLayout>

                <!--<Button-->
                <!--android:id="@+id/mainUI_authBtn"-->
                <!--android:layout_width="match_parent"-->
                <!--android:layout_height="32dp"-->
                <!--android:background="@drawable/block_bcg"-->
                <!--android:textSize="@dimen/sp14"-->
                <!--android:textColor="@color/white"-->
                <!--android:text="您还未认证，点我快速认证"-->
                <!--android:gravity="center"-->
                <!--android:layout_marginBottom="@dimen/dp15"/>-->

                <Button
                    android:id="@+id/mainUI_rechargeBtn"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp40"
                    android:background="@drawable/btn_bcg"
                    android:textSize="@dimen/sp14"
                    android:textColor="@color/white"
                    android:text="您的余额不足，点我快速充值"
                    android:gravity="center"
                    android:visibility="gone"/>

                <Button
                    android:id="@+id/mainUI_cartBtn"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp40"
                    android:background="@drawable/btn_bcg"
                    android:textSize="@dimen/sp14"
                    android:textColor="@color/white"
                    android:text="还剩XX天免费，每次前一个小时免费"
                    android:gravity="center"
                    android:visibility="gone"
                    android:layout_marginTop="@dimen/dp15"/>
            </RelativeLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_top_navi"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:visibility="gone"
            >

            <RelativeLayout
                android:id="@+id/rl_navi"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_below="@+id/rl_ad"
                android:background="@drawable/block_bcg"
                android:layout_marginTop="12dp"
                android:layout_marginLeft="18dp"
                android:layout_marginRight="18dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="0dp"
                    android:orientation="horizontal"
                    android:layout_alignParentLeft="true">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:layout_marginLeft="0dp"
                        android:orientation="vertical"
                        android:layout_alignParentLeft="true">

                        <TextView
                            android:id="@+id/tv_navi_name"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="0dp"
                            android:text="大学"
                            android:textColor="#FD555B"
                            android:textSize="@dimen/sp16"
                            />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="2dp"
                            android:text="可借车位置"
                            android:textColor="#666666"
                            android:textSize="@dimen/sp14"
                            />

                    </LinearLayout>
                    
                    <View
                        android:layout_width="1dp"
                        android:layout_height="26dp"
                        android:background="#C9C9C9"
                        />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:layout_marginLeft="0dp"
                        android:orientation="vertical"
                        android:layout_alignParentLeft="true">

                        <TextView
                            android:id="@+id/tv_navi_distance"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="0dp"
                            android:text="200米"
                            android:textColor="#FD555B"
                            android:textSize="@dimen/sp16"
                            />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="2dp"
                            android:text="距离"
                            android:textColor="#666666"
                            android:textSize="@dimen/sp14"
                            />

                    </LinearLayout>


                </LinearLayout>


            </RelativeLayout>

        </LinearLayout>



        <LinearLayout
            android:id="@+id/mainUI_scanCode_lock"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="30dp"
            android:orientation="horizontal"
            android:gravity="center"
            android:layout_alignParentBottom="true"
            >
            <ImageView
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/scan_icon"
                android:background="#00000000"
                android:layout_centerInParent="true"
                />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/mainUI_refreshLayout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_alignParentLeft="true"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="@dimen/dp30"
            android:layout_marginBottom="91dp"
            android:gravity="center"
            >

            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:src="@drawable/refresh_icon"
                android:scaleType="centerInside"/>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/mainUI_myLocationLayout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="25dp"
            android:layout_marginLeft="@dimen/dp30"
            android:orientation="vertical"
            android:gravity="center">

            <!--android:src="@drawable/btn_location"-->
            <!--android:src="@drawable/loading_large"-->

            <ImageView
                android:id="@+id/mainUI_myLocation7"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@null"
                android:src="@drawable/btn_location"
                android:layout_alignParentLeft="true"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingBottom="25dp"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginRight="@dimen/dp30"
            android:gravity="center">
            <LinearLayout
                android:id="@+id/mainUI_slideLayout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:gravity="center"
                android:layout_marginBottom="16dp">
                <ImageView
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:background="@null"
                    android:src="@drawable/slide_icon"
                    android:layout_alignParentRight="true"
                    android:scaleType="centerInside"/>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/mainUI_linkServiceLayout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="vertical">
                <ImageView
                    android:id="@+id/mainUI_linkService_btn"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:background="@null"
                    android:src="@drawable/link_service_icon"
                    android:layout_alignParentRight="true"
                    android:scaleType="centerInside"/>
            </LinearLayout>
        </LinearLayout>

    </RelativeLayout>



<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:melove="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/personUI_mainLayout"
    android:fitsSystemWindows="true"
    android:clipToPadding="true"
    android:background="#F5F5F5">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <include
            layout="@layout/ui_main_title"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="212dp"
            android:layout_marginTop="15dp"
            android:layout_marginLeft="@dimen/dp10"
            android:layout_marginRight="@dimen/dp10"
            android:orientation="vertical"
            android:background="@drawable/block_bcg">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:layout_marginTop="16dp"
                android:layout_marginLeft="12dp"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="10dp"
                    android:layout_height="10dp"
                    android:layout_marginTop="7dp"
                    android:background="@drawable/block_bcg1">
                </LinearLayout>


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="7dp"
                    android:textColor="#333333"
                    android:textSize="@dimen/sp18"
                    android:text="热门问题"/>

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <RelativeLayout
                    android:id="@+id/rl_name"
                    android:layout_width="match_parent"
                    android:layout_height="52dp"
                    android:layout_marginLeft="29dp"
                    android:layout_marginRight="23dp"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#333333"
                        android:textSize="@dimen/sp15"
                        android:text="怎么退余额"
                        android:layout_centerVertical="true"/>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:gravity="center_vertical">


                        <ImageView
                            android:layout_width="8dp"
                            android:layout_height="14dp"
                            android:src="@drawable/yjt4"
                            android:layout_marginLeft="11dp"/>
                    </LinearLayout>

                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/line_height"
                    android:background="#DDDDDD"
                    android:layout_marginLeft="29dp"
                    android:layout_marginRight="3dp"/>


                <RelativeLayout
                    android:id="@+id/rl_phoneNum"
                    android:layout_width="match_parent"
                    android:layout_height="52dp"
                    android:layout_marginLeft="29dp"
                    android:layout_marginRight="23dp"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#333333"
                        android:textSize="@dimen/sp15"
                        android:text="借不了车"
                        android:layout_centerVertical="true"/>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:gravity="center_vertical">


                        <ImageView
                            android:layout_width="8dp"
                            android:layout_height="14dp"
                            android:src="@drawable/yjt4"
                            android:layout_marginLeft="11dp"/>
                    </LinearLayout>
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/line_height"
                    android:background="#DDDDDD"
                    android:layout_marginLeft="29dp"
                    android:layout_marginRight="3dp"/>

                <RelativeLayout
                    android:id="@+id/rl_realNameAuth"
                    android:layout_width="match_parent"
                    android:layout_height="52dp"
                    android:layout_marginLeft="29dp"
                    android:layout_marginRight="23dp"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#333333"
                        android:textSize="@dimen/sp15"
                        android:text="还不了车"
                        android:layout_centerVertical="true"/>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:gravity="center_vertical">


                        <ImageView
                            android:layout_width="8dp"
                            android:layout_height="14dp"
                            android:src="@drawable/yjt4"
                            android:layout_marginLeft="11dp"/>
                    </LinearLayout>
                </RelativeLayout>

            </LinearLayout>


        </LinearLayout>



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="212dp"
            android:layout_marginTop="15dp"
            android:layout_marginLeft="@dimen/dp10"
            android:layout_marginRight="@dimen/dp10"
            android:orientation="vertical"
            android:background="@drawable/block_bcg">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:layout_marginTop="16dp"
                android:layout_marginLeft="12dp"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="10dp"
                    android:layout_height="10dp"
                    android:layout_marginTop="7dp"
                    android:background="@drawable/block_bcg1">
                </LinearLayout>


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="7dp"
                    android:textColor="#333333"
                    android:textSize="@dimen/sp18"
                    android:text="常见问题"/>

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <RelativeLayout
                    android:id="@+id/rl_name2"
                    android:layout_width="match_parent"
                    android:layout_height="52dp"
                    android:layout_marginLeft="29dp"
                    android:layout_marginRight="23dp"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#333333"
                        android:textSize="@dimen/sp15"
                        android:text="费用相关"
                        android:layout_centerVertical="true"/>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:gravity="center_vertical">


                        <ImageView
                            android:layout_width="8dp"
                            android:layout_height="14dp"
                            android:src="@drawable/yjt4"
                            android:layout_marginLeft="11dp"/>
                    </LinearLayout>

                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/line_height"
                    android:background="#DDDDDD"
                    android:layout_marginLeft="29dp"
                    android:layout_marginRight="3dp"/>


                <RelativeLayout
                    android:id="@+id/rl_phoneNum2"
                    android:layout_width="match_parent"
                    android:layout_height="52dp"
                    android:layout_marginLeft="29dp"
                    android:layout_marginRight="23dp"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#333333"
                        android:textSize="@dimen/sp15"
                        android:text="借还车相关"
                        android:layout_centerVertical="true"/>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:gravity="center_vertical">


                        <ImageView
                            android:layout_width="8dp"
                            android:layout_height="14dp"
                            android:src="@drawable/yjt4"
                            android:layout_marginLeft="11dp"/>
                    </LinearLayout>
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/line_height"
                    android:background="#DDDDDD"
                    android:layout_marginLeft="29dp"
                    android:layout_marginRight="3dp"/>

                <RelativeLayout
                    android:id="@+id/rl_realNameAuth2"
                    android:layout_width="match_parent"
                    android:layout_height="52dp"
                    android:layout_marginLeft="29dp"
                    android:layout_marginRight="23dp"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#333333"
                        android:textSize="@dimen/sp15"
                        android:text="注册账户相关"
                        android:layout_centerVertical="true"/>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:gravity="center_vertical">


                        <ImageView
                            android:layout_width="8dp"
                            android:layout_height="14dp"
                            android:src="@drawable/yjt4"
                            android:layout_marginLeft="11dp"/>
                    </LinearLayout>
                </RelativeLayout>

            </LinearLayout>


        </LinearLayout>


    </LinearLayout>




</RelativeLayout>





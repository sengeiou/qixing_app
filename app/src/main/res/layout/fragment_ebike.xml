<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:clipToPadding="true"
    android:orientation="vertical">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <LinearLayout
            android:id="@+id/mainUI_marqueeLayout2"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp25"
            android:gravity="center"
            android:background="#f57752"
            android:layout_marginTop="@dimen/dp5"
            android:orientation="horizontal"
            android:visibility="gone">

            <TextView
                android:id="@+id/mainUI_marquee2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="还车须至校内地图绿色区域，封顶5元/天"
                android:textColor="@color/white"
                android:textSize="@dimen/sp10"
                android:maxLines="1"
                android:ellipsize="end"
                android:marqueeRepeatLimit="marquee_forever"/>
        </LinearLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@id/mainUI_titleLayout">

            <!--<com.amap.api.maps.TextureMapView-->
                <!--android:id="@+id/mainUI_map2"-->
                <!--android:layout_width="match_parent"-->
                <!--android:layout_height="match_parent"-->
                <!--android:layout_alignParentTop="true"-->
                <!--android:layout_alignParentStart="true">-->
            <!--</com.amap.api.maps.TextureMapView>-->

            <LinearLayout
                android:id="@+id/personUI_bottom_billing_myCommissionLayout2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginRight="16dp"
                android:layout_marginBottom="277dp"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:gravity="center">

                <ImageView
                    android:layout_width="32dp"
                    android:layout_height="34dp"
                    android:src="@drawable/vip_center_icon"
                    android:layout_alignParentLeft="true"/>

            </LinearLayout>


            <LinearLayout
                android:id="@+id/mainUI_myLocationLayout2"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="24dp"
                android:layout_marginLeft="16dp"
                android:orientation="vertical"
                android:background="@drawable/back_bd"
                android:gravity="center">

                <ImageView
                    android:id="@+id/mainUI_myLocation2"
                    android:layout_width="22dp"
                    android:layout_height="22dp"
                    android:background="@null"
                    android:src="@drawable/btn_location"
                    android:layout_alignParentLeft="true"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingBottom="24dp"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_marginRight="16dp"
                android:gravity="center">
                <LinearLayout
                    android:id="@+id/mainUI_slideLayout2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:background="@drawable/back_bd"
                    android:layout_marginBottom="24dp">
                    <ImageView
                        android:layout_width="22dp"
                        android:layout_height="22dp"
                        android:background="@null"
                        android:src="@drawable/slide_icon"
                        android:layout_alignParentRight="true"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/mainUI_linkServiceLayout2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/back_bd"
                    android:gravity="center"
                    android:orientation="vertical">
                    <ImageView
                        android:id="@+id/mainUI_linkService_btn2"
                        android:layout_width="22dp"
                        android:layout_height="22dp"
                        android:background="@null"
                        android:src="@drawable/link_service_icon"
                        android:layout_alignParentRight="true"/>
                </LinearLayout>
            </LinearLayout>


            <LinearLayout
                android:id="@+id/mainUI_scanCode_lock2"
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:layout_marginLeft="83dp"
                android:layout_marginRight="83dp"
                android:layout_marginBottom="27dp"
                android:orientation="horizontal"
                android:background="@drawable/btn_bcg_scan_gradient"
                android:gravity="center"
                android:layout_alignParentBottom="true"
                >
                <ImageView
                    android:layout_width="19dp"
                    android:layout_height="19dp"
                    android:src="@drawable/scan"
                    android:background="#00000000"
                    android:layout_centerInParent="true"
                    />
                <TextView
                    android:id="@+id/tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:text="扫码开锁"
                    android:textSize="@dimen/sp18"
                    android:textColor="@color/white"
                    android:layout_centerInParent="true"
                    />
            </LinearLayout>


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingLeft="@dimen/dp20"
                android:paddingRight="@dimen/dp20"
                android:paddingTop="@dimen/dp15"
                android:visibility="gone"
                >

                <Button
                    android:id="@+id/mainUI_authBtn2"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp40"
                    android:background="@drawable/btn_bcg"
                    android:textSize="@dimen/sp14"
                    android:textColor="@color/white"
                    android:text="您还未认证，点我快速认证"
                    android:gravity="center"
                    android:visibility="gone"
                    android:layout_marginBottom="@dimen/dp15"/>

                <Button
                    android:id="@+id/mainUI_rechargeBtn2"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp40"
                    android:background="@drawable/btn_bcg"
                    android:textSize="@dimen/sp14"
                    android:textColor="@color/white"
                    android:text="您的余额不足，点我快速充值"
                    android:gravity="center"
                    android:visibility="gone"
                    android:layout_below="@id/mainUI_authBtn2"/>

                <Button
                    android:id="@+id/mainUI_cartBtn2"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp40"
                    android:background="@drawable/btn_bcg"
                    android:textSize="@dimen/sp14"
                    android:textColor="@color/white"
                    android:text="还剩XX天免费，每次前一个小时免费"
                    android:gravity="center"
                    android:visibility="gone"
                    android:layout_below="@id/mainUI_rechargeBtn2"
                    android:layout_marginTop="@dimen/dp15"/>
            </RelativeLayout>

            <LinearLayout
                android:id="@+id/rl_ad"
                android:layout_width="match_parent"
                android:layout_height="75dp"
                android:layout_marginLeft="13dp"
                android:layout_marginRight="13dp"
                android:layout_marginTop="15dp"
                android:background="@drawable/block_bcg"
                >


                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@drawable/ad"/>


            </LinearLayout>

            <RelativeLayout
                android:id="@+id/rl_authBtn"
                android:layout_width="match_parent"
                android:layout_height="37dp"
                android:layout_marginLeft="13dp"
                android:layout_marginRight="13dp"
                android:layout_marginTop="101dp"
                android:background="@drawable/block_bcg"
                >

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="13dp"
                    android:orientation="horizontal"
                    android:layout_alignParentLeft="true">

                    <ImageView
                        android:layout_width="21dp"
                        android:layout_height="21dp"
                        android:src="@drawable/horn"/>

                    <TextView
                        android:id="@+id/tv_authBtn"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="消息"
                        android:textColor="#666666"
                        android:textSize="@dimen/sp15"
                        android:layout_marginLeft="7dp"/>
                </LinearLayout>


                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:layout_marginRight="8dp"
                    android:orientation="horizontal"
                    android:layout_alignParentRight="true">

                    <ImageView
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:src="@drawable/yjt"/>
                </LinearLayout>

                <Button
                    android:id="@+id/mainUI_rechargeBtn"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp40"
                    android:background="@drawable/btn_bcg"
                    android:textSize="@dimen/sp14"
                    android:textColor="@color/white"
                    android:text="您的余额不足，点我快速充值"
                    android:gravity="center"
                    android:visibility="gone"/>

                <Button
                    android:id="@+id/mainUI_cartBtn"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp40"
                    android:background="@drawable/btn_bcg"
                    android:textSize="@dimen/sp14"
                    android:textColor="@color/white"
                    android:text="还剩XX天免费，每次前一个小时免费"
                    android:gravity="center"
                    android:visibility="gone"
                    android:layout_marginTop="@dimen/dp15"/>
            </RelativeLayout>

            <LinearLayout
                android:id="@+id/mainUI_refreshLayout2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginBottom="24dp"
                android:layout_alignParentLeft="true"
                android:layout_above="@id/mainUI_myLocationLayout2"
                android:layout_marginLeft="16dp"
                android:gravity="center"
                android:background="@drawable/back_bd"
                android:visibility="gone"
                >

                <ImageView
                    android:layout_width="22dp"
                    android:layout_height="22dp"
                    android:src="@drawable/refresh_icon"/>

            </LinearLayout>
        </RelativeLayout>
    </LinearLayout>
</LinearLayout>